<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainScreen.MainScreenController">
   <children>
      <Button fx:id="newGameButton" mnemonicParsing="false" onMouseClicked="#startNewGame" text="New game" />
      <Button mnemonicParsing="false" onMouseClicked="#loadGame" text="Load game" />
      <Button mnemonicParsing="false" onMouseClicked="#showOptions" text="Options" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
